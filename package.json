{
  "name": "blueshift-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "NEXT_PUBLIC_COMMIT_HASH=$(git rev-parse HEAD) next build",
    "dev": "next dev",
    "start": "next start",
    "lint": "next lint",
    "preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts",
    "generate-banners": "NEXT_PUBLIC_RELEASED_COURSES='*' NEXT_PUBLIC_RELEASED_CHALLENGES='*' ts-node -r tsconfig-paths/register -P ./tsconfig.scripts.json ./scripts/generate-banners.ts"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.31.1",
    "@mdx-js/loader": "^3.1.0",
    "@mdx-js/react": "^3.1.0",
    "@monaco-editor/react": "^4.7.0",
    "@next/mdx": "^15.3.0",
    "@next/third-parties": "^15.3.2",
    "@react-three/drei": "^10.1.2",
    "@react-three/fiber": "^9.1.2",
    "@solana/spl-token": "^0.4.13",
    "@solana/wallet-adapter-base": "^0.9.26",
    "@solana/wallet-adapter-phantom": "^0.9.27",
    "@solana/wallet-adapter-react": "^0.15.38",
    "@solana/wallet-adapter-react-ui": "^0.9.38",
    "@solana/wallet-adapter-solflare": "^0.6.31",
    "@solana/web3.js": "^1.98.1",
    "@tailwindcss/typography": "^0.5.16",
    "@tanstack/react-query": "^5.83.0",
    "@types/dat.gui": "^0.7.13",
    "@types/three": "^0.176.0",
    "bs58": "^6.0.0",
    "classnames": "^2.5.1",
    "dat.gui": "^0.7.9",
    "esbuild-wasm": "^0.25.4",
    "jose": "^6.0.11",
    "monaco-editor": "^0.52.2",
    "motion": "^12.6.3",
    "motion-plus": "^0.1.6",
    "next": "15.3.0",
    "next-intl": "^4.1.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "rehype-pretty-code": "^0.14.1",
    "three": "^0.177.0",
    "usehooks-ts": "^3.1.1",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@opennextjs/cloudflare": "^1.0.2",
    "@tailwindcss/postcss": "^4",
    "@types/mdx": "^2.0.13",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.3.0",
    "gray-matter": "^4.0.3",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5",
    "wrangler": "^4.22.0"
  }
